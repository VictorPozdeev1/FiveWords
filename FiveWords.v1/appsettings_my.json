{
  "JsonSerializerOptions": {
    "Internal": {
      "PropertyNameCaseInsensitive": "false", //"false" is default value
      "NumberHandling": "Strict" //"Strict" is default value
    },
    "Web": {
      "PropertyNameCaseInsensitive": "true",
      "NumberHandling": "AllowReadingFromString",
      "UseCamelCaseForPropertyNaming": "true", // Not a property name. Is processed "manually".
      "WriteIndented": "true"
    }
  }
}
